{
    "extends": "airbnb",
    "parserOptions":{
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "comma-dangle": "off",
        "indent": "off",
        "space-before-function-paren": "warn",
        "no-trailing-spaces": "off",
        "no-use-before-define": "off",
        "global-require": "off",
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": 0,
        "react/jsx-tag-spacing": 0,
        "react/jsx-indent-props": 0
    }
}